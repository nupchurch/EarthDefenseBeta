<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.ntu.way2fungames.earthdefensebeta" android:versionCode="1"
	android:versionName="1.0">
	
	<application android:name = "EarthDefenseBeta" android:icon="@drawable/icon" android:label="@string/app_name"
		android:debuggable="true">
		<activity android:label="@string/app_name" android:name="game"
			android:screenOrientation="portrait" android:multiprocess="false"
			android:configChanges="orientation|keyboardHidden" >
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
	</application>



	<supports-screens android:anyDensity="true" android:largeScreens="true" android:normalScreens="true"></supports-screens>
	<uses-sdk android:targetSdkVersion="11" android:minSdkVersion="6"></uses-sdk>

</manifest> 